{
  "name": "JSnippets",
  "version": "1.0",
  "description": "Handle and execute javascript snippets.",
  "permissions": [
    "storage",
    "declarativeContent",
    "http://*/*",
    "https://*/*",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
